        "**UPCOMING LEAP YEAR**"
      ],
      "metadata": {
        "id": "jDDC-mmxDf2p"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "year=int(input(\"Enter the year:\"))\n",
        "print(\"UPCOMING LEAP YEARS\")\n",
        "n=20\n",
        "c=0\n",
        "while(c!=n):\n",
        "    year+=1\n",
        "    if(year%4==0 and year%100!=0 or year%400==0):\n",
        "        c+=1\n",
        "        print(c,'',year)\n"
      ],
